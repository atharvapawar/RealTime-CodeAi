{
  "name": "realtime-ai-editor",
  "displayName": "RealTime AI Editor",
  "description": "AI-powered code assistant with daily-updated Next.js/Tailwind documentation",
  "version": "0.0.1",
  "publisher": "AtharvPawar",
  "repository": {
    "type": "git",
    "url": "https://github.com/atharvapawar/RealTime-CodeAi.git"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Extension Packs"
  ],
  "activationEvents": [
    "onCommand:realtime-ai-editor.activate"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "realtime-ai-editor.activate",
        "title": "Open RealTime AI Assistant"
      }
    ],
    "keybindings": [
      {
        "command": "realtime-ai-editor.activate",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^1.5.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0"
  },
  "icon": "icon.png",
  "license": "MIT"
}