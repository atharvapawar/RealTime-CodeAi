{
  "name": "realtime-ai-editor",
  "displayName": "RealTime AI Editor",
  "description": "AI-powered code assistant with daily-updated Next.js/Tailwind documentation",
  "version": "0.0.2",
  "publisher": "AtharvPawar",
  "repository": {
    "type": "git",
    "url": "https://github.com/atharvapawar/RealTime-CodeAi.git"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Extension Packs"
  ],
  "activationEvents": [
    "onCommand:realtime-ai-editor.openAssistant",
    "onCommand:realtime-ai-editor.refreshDocs",
    "onCommand:realtime-ai-editor.getContextualHelp"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "realtime-ai-editor.openAssistant",
        "title": "Open RealTime AI Assistant"
      },
      {
        "command": "realtime-ai-editor.refreshDocs",
        "title": "Refresh Documentation"
      },
      {
        "command": "realtime-ai-editor.getContextualHelp",
        "title": "Get Contextual AI Help"
      }
    ],
    "keybindings": [
      {
        "command": "realtime-ai-editor.openAssistant",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      },
      {
        "command": "realtime-ai-editor.getContextualHelp",
        "key": "ctrl+shift+h",
        "mac": "cmd+shift+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish",
    "watch": "code --extensionDevelopmentPath=."
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^1.8.1",
    "marked": "^11.1.1",
    "highlight.js": "^11.9.0",
    "@vscode/codicons": "^0.0.35"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0"
  },
  "icon": "icon.png",
  "license": "MIT"
}